; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Enhanced Inventory"
config/tags=PackedStringArray("addon")
run/main_scene="res://examples/equipment_inventory/EquipmentInventory.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.png"

[autoload]

EnhancedInventoryEventBus="*res://addons/enhanced_inventory/scripts/autoloads/enhanced_inventory_event_bus.gd"
ClassUtils="*res://addons/enhanced_common/scripts/utils/class_utils.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/enhanced_common/plugin.cfg", "res://addons/enhanced_inventory/plugin.cfg", "res://addons/enhanced_resource/plugin.cfg", "res://addons/zylann.editor_debugger/plugin.cfg")

[gui]

theme/custom="res://examples/_shared/resources/default.tres"

[rendering]

textures/canvas_textures/default_texture_filter=0
